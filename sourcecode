    <script type="text/processing" data-processing-target="optionsMode">
        var mode = "circles";
		
		void setup(){
            size(420,570);
            background(0,0,0);  
			
        }
        
        void draw(){
		
          //println(mode);
            stroke(0);
            fill(0,166,214);
            rect(350,0,100,28);
            if (mousePressed == true) {
                if (mouseX > 350){
                    if(mouseY < 28){
                    background(0,0,0);
                    }
                }
            }
      
            fill(255);
            textSize=(100);
            text("Refresh",365,20);
		
		fill(237, 17, 17);		
		rect(5,0,50,28);
		

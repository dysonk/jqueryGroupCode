		rect(5,180,50,28);
			
		fill(233, 237, 17);
		rect(5,210,50,28);
		
		if (mousePressed == true) {
		
		//HINT!!!!!!!! YOU CAN'T WRITE EXPRESSIONS LIKE THIS!!!! GOOGLE HOW TO WRITE CONDIDITIONALS!!
			if ((5 < mouseX && mouseX < 50) && (0 < mouseY && mouseY < 28)){
				mode = "circles";
				//println("I PRESSED CIRCLE");	
			}
		//HERE TOO!!!!!!!!!!!
			if ((5 < mouseX && mouseX < 50) && (30 < mouseY && mouseY < 58)){
				mode = "picasso";
				//println("I orESSED PCIASSO");
			}
			
			if ((5 < mouseX && mouseX < 50) && (60 < mouseY  && mouseY < 88)){
					mode = "strobe lights";
					//println("I pressed STROBE LIGHTS");
			}
			
			if ((5 < mouseX && mouseX < 50) && (90 < mouseY && mouseY < 118)){
					mode = "crazy paint";
					//println("I pressed CRAZY PAINT");
			}
			
			if ((5 < mouseX && mouseX < 50) && (120 < mouseY && mouseY < 148)) {
				mode = "spaced circles";
				//println("I pressed SPACED CIRCCLES");
			}
			

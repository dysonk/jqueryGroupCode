        function pausecomp(millis) 
        {
        var date = new Date();
        var curDate = null;

        do { curDate = new Date(); } 
        while(curDate-date < millis);
        }
    
        void mouseDragged() {
		
			if (mode == "circles"){
				
				drawCircles();
			}
			else if (mode == "picasso"){
				
				ongoingPicasso();
				
			}	
			else if (mode == "strobe lights"){
				strobeLights();
			}
			else if (mode == "crazy paint"){
				crazyPaint();
			}
			else if (mode == "spaced circles"){
				spacedCircles();
			}
			else if (mode == "color lines"){
				colorLinesCircles();
			}
			else if (mode == "splatter"){
				splatter();
			}
			else if (mode == "into the void"){
				intoTheVoid();
			}
        };
		
		void drawCircles(){
			var red = random(1,255);
			var green = random(1,255);
			var blue = random(1,255);
			var x = random(10,50);
			var y = random(10,50);
			pausecomp(40);
			var splatterx = random(1,50);
			var splattery = random(1,50);
			fill(red,green,blue);
			ellipse(mouseX + splatterx,mouseY + splattery,x,y);
		}
		
		void ongoingPicasso(){
		noStroke();
		//pauseComp(40);
		fill(mouseX, mouseY, random (0, 255));
		triangle(mouseX, mouseY, random (0, 400), random (0, 400), random (0, 400), random (0, 400));
		fill((mouseX - 50), (mouseY - 25), random (0, 255));
		ellipse (mouseX, mouseY, 50, 50);
		rect (mouseX, mouseY, 20, 20);
		}
		
		void strobeLights(){
			noStroke();
			var c1 = color(255, random(0, 255), random(0,255));
			//pauseComp(40);
			fill (c1);
			ellipse (mouseX, mouseY, 20, 20);
			beginShape();
			vertex (200, 200);
			bezierVertex(180, 100, 180, 175, mouseX, mouseY);
			bezierVertex(150, 180, 160, 125, mouseX, mouseY);
			endShape();
		}
		
		void crazyPaint(){
			frameRate(5);
			var color1 = random(0,255);
			var color2 = random(0,255);
			var color3 = random(0,255);
			fill(color1, color2, color3);
			stroke(color1, color2, color3);

			//pauseComp(40);
			strokeWeight(random(1,5));
			line(pmouseX, pmouseY, mouseX, mouseY);
		}
		
		
		var spacedCircles(){
			noStroke();

			var randColor = function() {
			var randR =  random(0,255);
			var randG =  random(0,255);
			var randB =  random(0,255);
			fill(randR, randG, randB);
			}			
			var randDraw = random(0,10);
			for (var n = 0; n < randDraw; n++) {
			randColor();
			var circ = random(5,20); //random diameter
			var randX = mouseX + random(-20, 20); //relative to mouseX
			var randY = mouseY + random(-20, 20); //relative to mouseY
			//pauseComp(40);
			ellipse(randX,randY,circ,circ);   
			}
		}
		
		
		void colorLinesCircles(){
			
			var x = random(5, 30);
			var y = random(5, 10);
			var z = random(5, 20);
			var randX = random(-20,20);
			var randY = random(-20,20);
			var randR = random(0, 255);
			var randG = random(0, 255);
			var randB = random(0, 255);
			//pauseComp(40);
			fill(randR, randG, randB);
			ellipse(mouseX, mouseY, x, x);
			ellipse(mouseX + randX, mouseY + randY, y, y);
			ellipse(mouseX - randX, mouseY - randY, z, z);
    
			stroke(randR, randG, randB);
			strokeWeight(1);
			line(200,200,mouseX, mouseY);
			
		}
		
		
		
		var splatter(){
			frameRate(5);
			var h = random(19,29);
			var change = random(5,100);
			var change2 = random(7,18);
			var change3 = random(7,18)
			var change4 = random(7,18);
			var change5 = random(7,18);
			var change6 = random(7,18);
			var change7 = random(7,18);
			var change8 = random(5,100);
			var change9 = random(5,100);
			var change10 = random(5,100);
			var change11 = random(5,100);
			var change12 = random(5,100);
			var change13 = random(5,100);
			var change14 = random(5,100);
			var r = random(40,255);
			var g = random(40,255);
			var b = random(40,255);
			//pauseComp(40);
			stroke(r,g,b);
			fill(r,g,b);
			ellipse(mouseX,mouseY,h,h);
			ellipse(mouseX-change,mouseY-change,h-change2,h-change2);
			ellipse(mouseX+change8,mouseY+change8,h-change3,h-change3);
			ellipse(mouseX-change9,mouseY+change9,h-change2,h-change2);
			ellipse(mouseX+change10,mouseY-change10,h-change3,h-change3);
			ellipse(mouseX+change11,mouseY,h-change4,h-change4);
			ellipse(mouseX-change12,mouseY,h-change5,h-change5);

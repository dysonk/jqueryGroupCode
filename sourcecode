    <link rel="stylesheet" href="http://code.jquery.com/mobile/1.0a4.1/jquery.mobile-1.0a4.1.min.css"/>
    <script src='https://cdn.firebase.com/js/client/1.0.15/firebase.js'></script>
    <script type="text/javascript" src="http://code.jquery.com/jquery-1.5.2.min.js"></script>
    <script type="text/javascript" src="http://code.jquery.com/mobile/1.0a4.1/jquery.mobile-1.0a4.1.min.js"></script>
    <script src="http://cloud.github.com/downloads/processing-js/processing-js/processing-1.4.1.min.js"></script>

      
 
    <script>
    
     // This creates the connection to the database
        var toDoDB = new Firebase('https://incandescent-torch-8399.firebaseio.com/');
        
        // This tells your web browser to respond when
        // SOMEONE (i.e. maybe not even you) adds to the database
        toDoDB.on('value', showItems);
        
           
        var nameofsongs=[];
        var nameofurl=[];
        var toDoDD;
        function showItems(snapshot){
            
            // Gets my data in my database as a list
            // Gets my data in my database as a list
            toDoDD=snapshot.val();
           
            console.log(toDoDD["Congratulations"]);
            
            // Loops over each identifier in my data base
            for(var identifier in toDoDD){
                console.log("ID: " + identifier  );
                nameofsongs.push(identifier);
                nameofurl.push(toDoDD[identifier]);
                console.log(nameofsongs);
                console.log(nameofurl);
            }
            
            
            
        }
        
        
        function fastest(){
            
            var thebox = ($("#songchoice3").val());
            
            for(var i=0; i< nameofsongs.length;i++){
                if (thebox==nameofsongs[i]){
            
            
                    
                    console.log(nameofurl[i]);
                
                
                    $("#fastestpage").attr("src", nameofurl[i]);
                
            
            
                }
            }
        }
        function slowest(){
            
            var thebox = ($("#songchoice1").val());
            
            
            
            console.log(thebox);
            
            for(var i=0; i< nameofsongs.length;i++){
                if (thebox==nameofsongs[i]){
            
            
                    
                    console.log(nameofurl[i]);
                
                
                    $("#slowestpage").attr("src", nameofurl[i]);
                
            
            
                }
            }
        }
        
        function computermodest(){
            
        
            console.log(thebox);
            console.log(nameofsongs[0]);
            var thebox = ($("#songchoice").val());
            
            
            for(var i=0; i< nameofsongs.length;i++){
                if (thebox==nameofsongs[i]){
            
            
                    
                    console.log(nameofurl[i]);
                
                
                    $("#computermodestpage").attr("src", nameofurl[i]);
                
            
            
                }
            }
        }
        function normalest(){
            
            
            var thebox = ($("#songchoice2").val());
        
            
            for(var i=0; i< nameofsongs.length;i++){
                if (thebox==nameofsongs[i]){
            
            
                    
                    console.log(nameofurl[i]);
                
                
                    $("#normalestpage").attr("src", nameofurl[i]);
                
            
            
                }
            }
        }
        
        
        
    /*var y="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/38336698&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false&amp;visual=true";
    var x="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/6301400&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false&amp;visual=true";
